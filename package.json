{
  "name": "povio-automation",
  "version": "1.0.0",
  "description": "Cypress automation tests for Povio internal app",
  "author": "QA Engineer",
  "scripts": {
    "test": "cypress run",
    "test:healthcheck": "cypress run --spec 'cypress/e2e/healthcheck.cy.js'",
    "test:registration": "cypress run --spec 'cypress/e2e/registration.cy.js'",
    "test:campaigns": "cypress run --spec 'cypress/e2e/campaigns.cy.js'",
    "open": "cypress open",
    "lint": "eslint cypress/**/*.js",
    "lint:fix": "eslint cypress/**/*.js --fix"
  },
  "devDependencies": {
    "cypress": "^14.5.0",
    "dotenv": "^16.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "mochawesome": "^7.1.3",
    "xlsx": "^0.18.5"
  }
}
